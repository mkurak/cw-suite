{
  "name": "cw.core",
  "version": "0.1.0",
  "description": "Aggregated core utilities for cw API projects.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check"
  },
  "dependencies": {
    "cw.api.core": "workspace:^",
    "cw.api.core.cache.memory": "workspace:^",
    "cw.api.core.db.typeorm": "workspace:^",
    "cw.api.core.di": "workspace:^",
    "cw.api.core.events": "workspace:^",
    "cw.api.core.queue.local": "workspace:^",
    "cw.helper.colored.console": "workspace:^",
    "cw.helper.dev.runner": "workspace:^",
    "cw.helper.package.generator": "workspace:^"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "cw",
    "api",
    "core",
    "typescript"
  ]
}
