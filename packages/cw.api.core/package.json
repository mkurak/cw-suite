{
  "name": "@cw-suite/api-core",
  "version": "0.2.1",
  "description": "Core Express server utilities for cw API projects.",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "predev:w": "npm run build",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.ts\" \"tests/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" \"tests/**/*.ts\" --check",
    "test:coverage": "jest --coverage",
    "dev:w": "cw-dev-runner --config ./cw-dev-runner.config.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "@cw-suite/helper-dev-runner": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mkurak/cw-suite.git",
    "directory": "packages/@cw-suite/api-core"
  },
  "bugs": {
    "url": "https://github.com/mkurak/cw-suite/issues"
  },
  "homepage": "https://github.com/mkurak/cw-suite#readme"
}
